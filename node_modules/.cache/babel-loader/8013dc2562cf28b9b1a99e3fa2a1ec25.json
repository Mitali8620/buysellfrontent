{"ast":null,"code":"import firebase from \"firebase/app\";\nimport \"firebase/messaging\";\nexport default {\n  name: \"PsNotificationBox\",\n  components: {},\n\n  data() {\n    return {\n      title: \"Test\",\n      subject: \"Test\",\n      messaging: firebase.messaging.isSupported() ? firebase.messaging() : null,\n      currentMessage: \"Test\",\n      notify: false\n    };\n  },\n\n  methods: {\n    closed() {},\n\n    receiveMessage() {\n      try {\n        if (!this.messaging) return;\n        this.messaging.onMessage(payload => {\n          // debugger\n          this.currentMessage = payload;\n          console.log(this.currentMessage);\n          console.log(\"Message received. \", payload);\n          this.setNotificationBoxForm(payload.notification.title, payload.notification.body);\n          this.notify = true;\n          setTimeout(() => {\n            this.notify = false;\n          }, 10000);\n        });\n      } catch (e) {\n        console.log(e);\n      }\n    },\n\n    setNotificationBoxForm(title, subject) {\n      this.title = title;\n      this.subject = subject;\n    }\n\n  },\n\n  created() {\n    this.receiveMessage();\n  },\n\n  validations: function () {\n    return {};\n  },\n  computed: {}\n};","map":{"version":3,"sources":["/Volumes/Parth Desai/buysellfrontent/src/components/core/notificationbox/PsNotificationBox.vue"],"names":[],"mappings":"AAqBA,OAAO,QAAP,MAAqB,cAArB;AACA,OAAO,oBAAP;AACA,eAAe;AACX,EAAA,IAAI,EAAE,mBADK;AAGX,EAAA,UAAU,EAAE,EAHD;;AAKX,EAAA,IAAI,GAAG;AACH,WAAO;AACH,MAAA,KAAK,EAAE,MADJ;AAEH,MAAA,OAAO,EAAE,MAFN;AAGH,MAAA,SAAS,EAAE,QAAQ,CAAC,SAAT,CAAmB,WAAnB,KAAmC,QAAQ,CAAC,SAAT,EAAnC,GAA0D,IAHlE;AAIH,MAAA,cAAc,EAAE,MAJb;AAKH,MAAA,MAAM,EAAE;AALL,KAAP;AAOH,GAbU;;AAeX,EAAA,OAAO,EAAE;AACL,IAAA,MAAM,GAAE,CAEP,CAHI;;AAIL,IAAA,cAAc,GAAG;AACb,UAAI;AACA,YAAG,CAAC,KAAK,SAAT,EAAoB;AACpB,aAAK,SAAL,CAAe,SAAf,CAA0B,OAAD,IAAa;AAClC;AACA,eAAK,cAAL,GAAsB,OAAtB;AACA,UAAA,OAAO,CAAC,GAAR,CAAY,KAAK,cAAjB;AACA,UAAA,OAAO,CAAC,GAAR,CAAY,oBAAZ